{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(favicon.ico|sitemap.xml)",
      "dest": "/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
